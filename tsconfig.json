{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "strictNullChecks": false,
        "noImplicitThis": false
    },
    "include": [
        "./src/**/*"
    ]
}
